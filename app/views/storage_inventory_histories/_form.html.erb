<%= simple_form_for @storage_inventory_history, :html => { :class => 'well form-horizontal' } do |f| %>
  <%= f.input :occurred_date %>
  <%= f.input :slip_code %>
  <%= f.input :purchase_order_datum_id %>
  <%= f.input :construction_datum_id %>
  <%= f.input :material_master_id %>
  <%= f.input :quantity %>
  <%= f.input :unit_price %>
  <%= f.input :amount %>
  <%= f.input :supplier_master_id %>
  <%= f.input :inventory_division_id %>
  
  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                storage_inventory_histories_path, :class => 'btn btn-default' %>
<% end %>
