<%= simple_form_for @storage_inventory_history, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :occurred_date %>
  <%= error_span(@storage_inventory_history[:occurred_date]) %>
  <%= f.input :slip_code %>
  <%= error_span(@storage_inventory_history[:slip_code]) %>
  <%= f.input :purchase_order_datum_id %>
  <%= error_span(@storage_inventory_history[:purchase_order_datum_id]) %>
  <%= f.input :construction_datum_id %>
  <%= error_span(@storage_inventory_history[:construction_datum_id]) %>
  <%= f.input :material_master_id %>
  <%= error_span(@storage_inventory_history[:material_master_id]) %>
  <%= f.input :quantity %>
  <%= error_span(@storage_inventory_history[:quantity]) %>
  <%= f.input :unit_price %>
  <%= error_span(@storage_inventory_history[:unit_price]) %>
  <%= f.input :amount %>
  <%= error_span(@storage_inventory_history[:amount]) %>
  <%= f.input :supplier_master_id %>
  <%= error_span(@storage_inventory_history[:supplier_master_id]) %>
  <%= f.input :invenrtory_division_id %>
  <%= error_span(@storage_inventory_history[:invenrtory_division_id]) %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                storage_inventory_histories_path, :class => 'btn btn-default' %>
<% end %>
